{
    "manifest_version":2,
    "name":"VKFuncs",
    "description":"данное расширение фильтрует записи в ленте.",
    "version":"1.0",
    "permissions":[
        "activeTab",
        "tabs"
    ],
    "browser_action": {
        "default_title":"VKFuncs",
        "default_icon":"VK_Filter_Logo.png",
        "default_popup":"popup.html"
    },
    "icons": {
        "16":"VK_Filter_Logo.png",
        "32":"VK_Filter_Logo.png",
        "48":"VK_Filter_Logo.png",
        "128":"VK_Filter_Logo.png"
    }
}